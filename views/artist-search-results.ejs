<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/stylesheets/style.css">
</head>

<body>
    <% artist.forEach(elt => { %>
    <div class="artist">
        <h1> <%= elt.name%> </h1>
        <% elt.images.forEach(element => { %>
        <img src="<%= element.url%>" alt="">
        <% }) %>
        <a href="/albums/<%= elt.id%>">View Albums</a>
    </div>
    <% }) %>

</body>

</html>